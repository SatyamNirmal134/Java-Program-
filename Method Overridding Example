class A 
{
    void roi()
    {
        System.out.println("Rate of Interest is: 9%");
    }
}
class B extends A 
{
    void roi()
    {
        System.out.println("Rate of Interest is: 9%");
    }
}
class C extends A 
{
    void roi()
    {
        System.out.println("Rate of Interest is: 9.5%");
    }
}



public class Main
{
	public static void main(String[] args)
	{
		
		B b=new B();
		b.roi();
		C c=new C();
		c.roi();
	}
}
